{
  "Logging": {
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Overrides": {
      "Microsoft": "Warning"
    }
  },
  "Quartz": {
    "SchedulerId": "AccountingJobScheduler",
    "SchedulerName": "AccountingJobScheduler",
    "MisfireThreshold": "0:00:05",
    "Scheduling": {},
    "JobFactory": {}
  }
}